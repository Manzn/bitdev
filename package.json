    "styled-jsx-plugin-sass": "^1.0.0",
    "ts-md5": "^1.2.7"
  },
  "devDependencies": {
    "@babel/core": "^7.10.2",
    "@types/jest": "^26.0.0",
    "@types/next": "^9.0.0",
